{
    "name": "smbplus/usermanagement",
    "description": "UI for managing Laravel Users with Spatie Permissions",
    "type": "project",
    "require": {
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "orchestra/testbench-core": "7.0"
    },
    "autoload": {
        "psr-4": {
            "Smbplus\\UserManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Smbplus\\UserManagement\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smbplus\\UserManagement\\UserManagementServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ntuanphuc",
            "email": "ntuanphuc@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
